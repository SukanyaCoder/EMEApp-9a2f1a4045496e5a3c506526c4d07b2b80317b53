<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nuttygeek.com.eme">

    <application
        android:allowBackup="true"
        android:icon="@drawable/eme_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/eme_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="nuttygeek.com.eme.MainActivity" />
        <activity android:name="nuttygeek.com.eme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nuttygeek.com.eme.MainGrid"
            android:label="@string/title_activity_main_grid"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="nuttygeek.com.eme.MainCompanyProfile" />
        <activity android:name="nuttygeek.com.eme.MainSeeCourses" />
        <activity android:name="nuttygeek.com.eme.MainVideos" />
        <activity android:name="nuttygeek.com.eme.MainContact" />
        <activity android:name="nuttygeek.com.eme.MainAdmission" />
        <activity android:name="nuttygeek.com.eme.WelcomeActivity" />
        <activity android:name="nuttygeek.com.eme.MainCourses" />
        <activity android:name="nuttygeek.com.eme.MainDifferentCourses" />
        <activity android:name="nuttygeek.com.eme.MainAuthentication"></activity>
    </application>

</manifest>